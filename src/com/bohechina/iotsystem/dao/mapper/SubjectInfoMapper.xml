<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bohechina.iotsystem.dao.SubjectInfoDao">

	<select id="getSubjectList" resultType="SubjectInfo">
		select * from subjectInfo
	</select>
	
	<select id="getSubjectInfoById" resultType="SubjectInfo" parameterType="String">
		select * from subjectInfo
		where id=#{subject}
	</select>

</mapper>